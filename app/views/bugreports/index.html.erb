<h1>Listing bugreports</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Version</th>
    <th>Hardware</th>
    <th>Beschreibung</th>
  </tr>

<% @bugreports.each do |bugreport| %>
  <tr>
    <td><%=h bugreport.name %></td>
    <td><%=h bugreport.version %></td>
    <td><%=h bugreport.hardware %></td>
    <td><%=h bugreport.beschreibung %></td>
    <td><%= link_to 'Show', bugreport %></td>
    <td><%= link_to 'Edit', edit_bugreport_path(bugreport) %></td>
    <td><%= link_to 'Destroy', bugreport, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New bugreport', new_bugreport_path %>